{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/theme/*": [
        "src/theme/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/types": [
        "src/types/index"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/screens/*": [
        "src/screens/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/services/*": [
        "src/services/*"
      ],
      "@/config/*": [
        "src/config/*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}